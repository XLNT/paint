{
  "name": "paint",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "start-storybook -p 9009 -s public --ci",
    "build": "NODE_ENV=production yarn babel src -d dist --extensions '.ts,.tsx,.js,.jsx' --source-maps inline",
    "build:storybook": "build-storybook -s public -o build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "yarn eslint 'src/**/*.{js,jsx,ts,tsx,json,mdx}'"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,mdx}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@react-aria/button": "^3.0.2",
    "@react-aria/focus": "^3.0.2",
    "@react-aria/interactions": "^3.0.2",
    "@react-aria/utils": "^3.0.2",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "classnames": "^2.2.6",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-merge-refs": "^1.0.0",
    "react-scripts": "3.4.1",
    "react-use": "^15.3.2",
    "typescript": "~3.7.2",
    "webfontloader": "^1.6.28"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@react-types/button": "^3.0.2",
    "@storybook/addon-a11y": "^5.3.19",
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-info": "^5.3.19",
    "@storybook/addon-knobs": "^5.3.19",
    "@storybook/addon-links": "^5.3.19",
    "@storybook/addon-viewport": "^5.3.19",
    "@storybook/addons": "^5.3.19",
    "@storybook/preset-create-react-app": "^3.1.3",
    "@storybook/react": "^5.3.19",
    "@types/classnames": "^2.2.10",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/webfontloader": "^1.6.32",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "postcss-import": "^12.0.1",
    "postcss-nested": "^4.2.2",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.0.5",
    "storybook-addon-designs": "^5.4.0",
    "tailwindcss": "^1.4.6",
    "tailwindcss-plugins": "^0.3.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
